hange={this.handleChange}\r\n                        label='Display Name'\r\n                        required\r\n                    />\r\n\r\n                    <FormInput\r\n                        type='email'\r\n                        name='email'\r\n                        value={email}\r\n                        onChange={this.handleChange}\r\n                        label='Email'\r\n                        required\r\n                    />\r\n\r\n                    <FormInput\r\n                        type='password'\r\n                        name='password'\r\n                        value={password}\r\n                        onChange={this.handleChange}\r\n                        label='Password'\r\n                        required\r\n                    />\r\n\r\n                    <FormInput\r\n                        type='password'\r\n                        name='confirmPassword'\r\n                        value={confirmPassword}\r\n                        onChange={this.handleChange}\r\n                        label='Confirm Password'\r\n                        required\r\n                    />\r\n\r\n                    <CustomButton type='submit'>\r\n                        SIGN UP\r\n                    </CustomButton>\r\n\r\n                </form>\r\n            </div>\r\n        );\r\n    }\r\n\r\n}\r\n\r\nexport default SignUp;\r\n\r\n\r\n\r\n","C:\\JavaScript\\REACT\\crwn-clothing\\src\\redux\\store.js",[],"C:\\JavaScript\\REACT\\crwn-clothing\\src\\redux\\root-reducer.js",[],"C:\\JavaScript\\REACT\\crwn-clothing\\src\\redux\\user\\user.actions.js",[],"C:\\JavaScript\\REACT\\crwn-clothing\\src\\redux\\user\\user.reducer.js",[],"C:\\JavaScript\\REACT\\crwn-clothing\\src\\redux\\user\\user.types.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":37,"column":19,"nodeType":"97","messageId":"98","endLine":37,"endColumn":26},"no-native-reassign",["99"],"no-negated-in-